{
  "name": "promptreviews",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "predev": "node scripts/check-migrations.js",
    "dev": "DISABLE_SENTRY=true next dev -p 3002",
    "dev:webpack": "DISABLE_SENTRY=true next dev -p 3002",
    "dev:debug": "next dev -p 3002",
    "dev:clean": "npm run safety:full-audit && npm run dev",
    "cleanup": "rm -rf .next && rm -rf node_modules/.cache",
    "safety:full-audit": "npm audit && npm audit fix",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build -f next.config.analyzer.js",
    "analyze": "npm run build:analyze",
    "lint": "next lint",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:status": "npx supabase status",
    "debug:auth": "node scripts/debug-auth-issues.js",
    "test:auth": "node scripts/test-auth-flow.js",
    "test:auth:comprehensive": "node scripts/test-auth-comprehensive.js",
    "auth:full-check": "npm run debug:auth && npm run test:auth:comprehensive",
    "audit:supabase-clients": "node scripts/audit-supabase-clients.js",
    "migrate:supabase-clients": "node scripts/migrate-supabase-clients.js",
    "build:widget": "terser public/widgets/multi/widget-embed.js -o public/widgets/multi/widget-embed.min.js --compress --mangle && terser public/widgets/single/widget-embed.js -o public/widgets/single/widget-embed.min.js --compress --mangle && terser public/widgets/photo/widget-embed.js -o public/widgets/photo/widget-embed.min.js --compress --mangle",
    "build:multi-widget": "terser public/widgets/multi/widget-embed.js -o public/widgets/multi/widget-embed.min.js --compress --mangle",
    "build:single-widget": "terser public/widgets/single/widget-embed.js -o public/widgets/single/widget-embed.min.js --compress --mangle",
    "setup:stripe-coupons": "node scripts/setup-simplified-coupons.js",
    "test:reactivation": "node scripts/test-reactivation-flow.js",
    "build:photo-widget": "node scripts/build-photo-widget.js && terser public/widgets/photo/widget-embed.temp.js -o public/widgets/photo/widget-embed.min.js --compress --mangle && rm public/widgets/photo/widget-embed.temp.js",
    "watch:widget": "nodemon --watch public/widgets/multi/widget-embed.js --watch public/widgets/single/widget-embed.js --watch public/widgets/photo/widget-embed.js --exec 'npm run build:widget'",
    "migrations:check": "node scripts/check-migrations.js",
    "migrations:apply": "supabase db migrate",
    "check-migrations": "node scripts/check-migrations.js",
    "performance:test": "node scripts/performance-monitor.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:analyze": "node scripts/performance-optimizer.js",
    "performance:optimize": "npm run performance:analyze && npm run build",
    "production:monitor": "node scripts/production-monitor.js",
    "production:health": "node scripts/production-monitor.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@sentry/nextjs": "^9.31.0",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "@types/node": "22.15.17",
    "@types/pg": "^8.15.4",
    "@types/qrcode": "^1.5.5",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "eslint-config-next": "15.3.2",
    "form-data": "^4.0.2",
    "ioredis": "^5.6.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "mailgun.js": "^12.0.1",
    "nanoid": "^5.1.5",
    "next": "^15.3.2",
    "openai": "^4.98.0",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "19.1.0",
    "react-easy-crop": "^5.4.1",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.1",
    "react-ziggeo": "^4.6.2",
    "recharts": "^2.15.3",
    "resend": "^4.5.1",
    "slugify": "^1.6.6",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.0",
    "typescript": "5.8.3",
    "ziggeo-client-sdk": "^2.43.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.53.2",
    "@rushstack/eslint-patch": "^1.11.0",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.14",
    "clean-css": "^5.3.3",
    "cross-env": "^10.0.0",
    "dotenv": "^16.6.1",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.0",
    "memory-leak-detector": "^0.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.31",
    "supabase": "^2.22.12",
    "tailwindcss": "^3.3.0",
    "terser": "^5.43.1"
  }
}
