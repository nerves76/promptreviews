{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true
    }
  },
  "functions": {
    "src/app/(app)/api/geo-grid/check/route.ts": {
      "maxDuration": 60
    },
    "src/app/(app)/api/cron/run-scheduled-geogrids/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/send-trial-reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/send-mid-trial-checkins",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/send-trial-expired",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/send-review-reminders",
      "schedule": "0 10 1 * *"
    },
    {
      "path": "/api/cron/process-google-business-scheduled",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/cron/cleanup-google-business-scheduled",
      "schedule": "0 14 15 * *"
    },
    {
      "path": "/api/cron/send-optimizer-emails",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/cron/health-monitor",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/post-monthly-stats",
      "schedule": "0 12 1 * *"
    },
    {
      "path": "/api/cron/calculate-daily-stats",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/cleanup-notifications",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/refresh-monthly-credits",
      "schedule": "0 23 28-31 * *"
    },
    {
      "path": "/api/cron/process-rss-feeds",
      "schedule": "30 * * * *"
    },
    {
      "path": "/api/cron/cleanup-expired-invitations",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/send-invitation-reminders",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/send-monthly-insights",
      "schedule": "0 9 2 * *"
    },
    {
      "path": "/api/cron/send-comeback-emails",
      "schedule": "0 11 15 * *"
    },
    {
      "path": "/api/cron/hourly-dispatcher",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/daily-8am-dispatcher",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/verify-google-reviews",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/process-all-batches",
      "schedule": "* * * * *"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/_next/static/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
